{
  "private": true,
  "name": "circleci-2-test",
  "main": "build/index",
  "typings": "build/index",
  "description": "A test repository to figure out how the circleci 2.0 config works.",
  "version": "0.0.4",
  "scripts": {
    "build": "tsc",
    "test": "tsc && nyc mocha --require source-map-support/register 'build/test/**.js'",
    "lint": "tslint --project .",
    "report-coverage": "# codeclimate-test-reporter < coverage/lcov.info"
  },
  "engines": {
    "node": ">=7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robojones/circleci-2-test.git"
  },
  "keywords": [
    "gulpplugin",
    "gulp",
    "browser",
    "require",
    "module",
    "web",
    "export"
  ],
  "author": "robojones",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robojones/circleci-2-test/issues"
  },
  "homepage": "https://github.com/robojones/circleci-2-test#readme",
  "dependencies": {
    "@types/node": "^9.6.4",
    "once-promise": "^1.0.1"
  },
  "directories": {
    "test": "build/test/"
  },
  "nyc": {
    "extension": [
      ".js"
    ],
    "include": [
      "build/**"
    ],
    "exclude": [
      "build/test/**"
    ],
    "reporter": [
      "lcov",
      "html",
      "text"
    ]
  },
  "devDependencies": {
    "@types/mocha": "^5.0.0",
    "codeclimate-test-reporter": "^0.5.0",
    "mocha": "^5.0.5",
    "nyc": "^11.6.0",
    "source-map-support": "^0.5.4",
    "tslint": "^5.9.1",
    "tslint-config-ultra": "^2.0.0",
    "typescript": "^2.8.1"
  }
}
