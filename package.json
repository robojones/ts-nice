{
  "name": "ts-tools",
  "main": "build/index",
  "typings": "build/index",
  "description": "Some types that make your life easier when using typescript.",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc",
    "test": "echo 'no tests'",
    "lint": "tslint --project .",
    "report-coverage": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "engines": {
    "node": ">=7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robojones/ts-tools.git"
  },
  "keywords": [
    "typescript",
    "type",
    "select",
    "update"
  ],
  "author": "robojones",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robojones/ts-tools/issues"
  },
  "homepage": "https://github.com/robojones/ts-tools#readme",
  "dependencies": {
    "@types/node": "^9.6.4",
    "once-promise": "^1.0.1"
  },
  "directories": {
    "test": "build/test/"
  },
  "nyc": {
    "extension": [
      ".js"
    ],
    "include": [
      "build/**"
    ],
    "exclude": [
      "build/test/**"
    ],
    "reporter": [
      "lcov",
      "html",
      "text"
    ]
  },
  "devDependencies": {
    "@types/mocha": "^5.0.0",
    "codeclimate-test-reporter": "^0.5.0",
    "mocha": "^5.0.5",
    "nyc": "^11.6.0",
    "source-map-support": "^0.5.4",
    "tslint": "^5.9.1",
    "tslint-config-ultra": "^2.0.0",
    "typescript": "^2.8.1"
  }
}
